{
    "extends": [
        "airbnb"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "arrow-parens": 0,
        "no-console": 0,

        "prettier/prettier": [
            "error",
            {
              "trailingComma": "es5",
              "singleQuote": true,
              "printWidth": 80
            }
          ],
          "jsx-a11y/href-no-hash": "off",
          "jsx-a11y/anchor-is-valid": [
            "warn",
            {
              "aspects": [
                "invalidHref"
              ]
            }
          ]
       
    }
    
}